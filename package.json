  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kapetacom/node-cluster-config.git"
  },
  "keywords": [
    "kapeta"
  ],
  "author": "Henrik Hofmeister <hh@kapeta.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kapetacom/node-cluster-config/issues"
  },
  "homepage": "https://github.com/kapetacom/node-cluster-config#readme",
  "scripts": {
    "test": "echo its fine"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "yaml": "1.6.0"
  },
  "prettier": {
    "tabWidth": 4,
    "singleQuote": true
  },
  "release": {
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/changelog",
            {
                "changelogFile": "CHANGELOG.md"
            }
        ],
        "@semantic-release/npm",
        "@semantic-release/github",
        [
            "@semantic-release/git",
            {
                "assets": [
                    "CHANGELOG.md",
                    "package.json",
                    "package-lock.json"
                ],
                "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
            }
        ]
    ]
  }
}
